{
  "name": "songkick-api-proxy",
  "version": "1.0.0",
  "description": "An Express based API proxy to get around the Songkick API's lack of CORS support",
  "main": "index.js",
  "scripts": {
    "dev": "yarn build && yarn start",
    "dev:watch": "yarn dev && nodemon src/**/*.js --exec yarn build",
    "build": "babel src --source-maps --out-dir=dist",
    "start": "npm run build && node ./dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint **/*.js --quiet"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.2",
    "color-logger": "^0.0.6",
    "es6-promise": "^4.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.3",
    "isomorphic-fetch": "^2.2.1",
    "jdu": "^1.0.0",
    "nodemon": "^1.11.0"
  }
}
